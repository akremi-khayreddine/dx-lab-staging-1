{
  "VERSION": "v0.0.0",
  "ENV": "staging",
  "PROJECT_TYPE": "lib",
  "PROJECT_NAME": "dx-lab",
  "APP_NAME": "${{ PROJECT_NAME }}",
  "DOMAIN_HOSTING": "web.app",
  "APP_SUB_DOMAIN": "${{ APP_NAME }}-${{ ENV }}",
  "APP_URL": "https://${{ APP_SUB_DOMAIN }}.${{ DOMAIN_HOSTING }}",
  "APP_DIST": "dist/${{ APP_NAME }}",
  "LIB_NAME": "${{ APP_NAME }}-lib",
  "LIB_DIST": "dist/${{ LIB_NAME }}",
  "FIREBASE_PROJECT": "elocatus-staging",
  "FIREBASE_TARGET": "${{ APP_NAME }}",
  "FIREBASE_HOSTING": "${{ APP_NAME }}-${{ ENV }}",
  "FIREBASE_HOSTING_URL": "${{ FIREBASE_HOSTING }}.web.app",
  "FIREBASE_DATABASE_URL": "${{ FIREBASE_PROJECT }}.firebaseio.com",
  "FIREBASE_STORAGE_BUCKET": "${{ FIREBASE_PROJECT }}.appspot.com",
  "FIREBASE_PREDEPLOY": "cp ${{ APP_CONFIG }} ${{ APP_DIST }}/assets/config.json",
  "FIREBASE_POSTDEPLOY": "curl -X POST -H 'Content-type: application/json' --data '${{ SLACK_MESSAGE }}' ${{ SLACK_WEBHOOK_URL }}",
  "NODE_VERSION": "12.x",
  "SECRETS_PATH": "./secrets",
  "SCRIPTS_PATH": ".github/scripts",
  "REPO_NAME": "dx-lab",
  "REPO_OWNER": "distinctlab",
  "GITHUB_URL": "https://github.com",
  "GITHUB_REGISTRY_URL": "https://npm.pkg.github.com/",
  "ACTIONS_RUN_URL": "${{ GITHUB_URL }}/${{ REPO_OWNER }}/${{ REPO_NAME }}/actions/runs",
  "ACTIONS_ARTIFACT_URL": "${{ GITHUB_URL }}/${{ REPO_OWNER }}/${{ REPO_NAME }}/actions/runs",
  "RELEASES_TAG_URL": "${{ GITHUB_URL }}/${{ REPO_OWNER }}/${{ REPO_NAME }}/releases/tag",
  "APP_CONFIG": "app-config.json",
  "API_CONFIG": "./service-account-key.json.gpg",
  "SLACK_WEBHOOK_URL": "https://distinctlab.slack.com/home",
  "SLACK_MESSAGE": "{text: $1 }",
  "ARTIFACT_NAME": "${{ APP_NAME }}-${{ VERSION }}.tar.gz",
  "ARTIFACT_URL": "${{ RELEASES_TAG_URL }}/${{ VERSION }}"
}